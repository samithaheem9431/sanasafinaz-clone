        * {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Poppins', sans-serif;
}

body {
    background: #0a192f;
    color: #8892b0;
    line-height: 1.7;
}
.contact-item a{
    text-decoration: none;
    color:  #ccd6f6;
}
.cv-container {
    display: grid;
    grid-template-columns: 350px 1fr;
    min-height: 100vh;
}

/* Sidebar Styles */
.sidebar {
    background: #112240;
    padding: 40px;
    position: fixed;
    width: 350px;
    height: 100vh;
    overflow-y: auto;
}

.profile-section {
    text-align: center;
    margin-bottom: 40px;
}

.profile-container {
    position: relative;
    width: 200px;
    height: 200px;
    margin: 0 auto 20px;
}

.profile-border {
    position: absolute;
    top: -10px;
    left: -10px;
    right: -10px;
    bottom: -10px;
    border: 2px solid #64ffda;
    border-radius: 50%;
    animation: rotate 10s linear infinite;
}

@keyframes rotate {
    from { transform: rotate(0deg); }
    to { transform: rotate(360deg); }
}

.profile-pic {
    width: 200px;
    height: 200px;
    border-radius: 50%;
    object-fit: cover;
    border: 4px solid #233554;
    cursor: pointer;
    transition: transform 0.3s ease;
}

.profile-pic:hover {
    transform: scale(1.05);
}

.name {
    color: #ccd6f6;
    font-size: 2em;
    margin: 20px 0 10px;
}

.title {
    color: #64ffda;
    font-size: 1.2em;
    margin-bottom: 20px;
}

.contact-info {
    background: #233554;
    padding: 20px;
    border-radius: 10px;
    margin-bottom: 30px;
}

.contact-item {
    display: flex;
    align-items: center;
    gap: 10px;
    margin-bottom: 15px;
    color: #ccd6f6;
}

/* Main Content Styles */
.main-content {
    margin-left: 350px;
    padding: 40px;
}

.floating-intro {
    display: none;
    position: fixed;
    top: 20px;
    right: 20px;
    width: 300px;
    background: #112240;
    padding: 20px;
    border-radius: 10px;
    border: 1px solid #64ffda;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.5);
    z-index: 1000;
    color: #ccd6f6;
    animation: slideIn 0.3s ease-out;
}

@keyframes slideIn {
    from {
        transform: translateX(50px);
        opacity: 0;
    }
    to {
        transform: translateX(0);
        opacity: 1;
    }
}

section {
    margin-bottom: 60px;
}

.section-title {
    color: #ccd6f6;
    font-size: 1.8em;
    margin-bottom: 30px;
    display: flex;
    align-items: center;
}

.section-title::before {
    content: '';
    display: inline-block;
    width: 50px;
    height: 2px;
    background: #64ffda;
    margin-right: 20px;
}

.experience-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
}

.experience-card {
    background: #112240;
    padding: 25px;
    border-radius: 10px;
    transition: transform 0.3s ease;
    cursor: pointer;
    border: 1px solid #233554;
}

.experience-card:hover {
    transform: translateY(-5px);
    border-color: #64ffda;
}

.experience-card h3 {
    color: #64ffda;
    margin-bottom: 10px;
}

.experience-card .date {
    color: #ccd6f6;
    font-size: 0.9em;
    margin-bottom: 15px;
}

.skills-container {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: 20px;
}

.skill-item {
    background: #112240;
    padding: 15px;
    border-radius: 10px;
    text-align: center;
    transition: transform 0.3s ease;
    border: 1px solid #233554;
}

.skill-item:hover {
    transform: translateY(-3px);
    border-color: #64ffda;
}

.skill-name {
    color: #ccd6f6;
    margin-bottom: 10px;
}

.skill-level {
    width: 100%;
    height: 6px;
    background: #233554;
    border-radius: 3px;
    overflow: hidden;
}

.skill-progress {
    height: 100%;
    background: #64ffda;
    border-radius: 3px;
    transition: width 1s ease;
}

.education-timeline {
    position: relative;
    padding-left: 30px;
}

.education-item {
    position: relative;
    margin-bottom: 30px;
    padding: 20px;
    background: #112240;
    border-radius: 10px;
    border: 1px solid #233554;
}

.education-item::before {
    content: '';
    position: absolute;
    left: -30px;
    top: 50%;
    width: 15px;
    height: 15px;
    background: #64ffda;
    border-radius: 50%;
    transform: translateY(-50%);
}

.education-item::after {
    content: '';
    position: absolute;
    left: -23px;
    top: 0;
    bottom: -30px;
    width: 1px;
    background: #64ffda;
}

.education-item:last-child::after {
    display: none;
}

.education-item h3 {
    color: #64ffda;
    margin-bottom: 10px;
}

.education-item .school {
    color: #ccd6f6;
    font-weight: 500;
}

.floating-intro {
    display: none;
    position: fixed;
    top: 20px;
    right: 20px;
    width: 300px;
    background: #112240;
    padding: 20px;
    border-radius: 10px;
    border: 1px solid #64ffda;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.5);
    z-index: 1000;
    color: #ccd6f6;
    opacity: 0;
    transform: translateX(50px);
    transition: opacity 0.3s ease, transform 0.3s ease;
}

.profile-pic {
    transition: transform 0.3s ease;
}

.profile-border {
    transition: animation 0.3s ease;
}

@media (max-width: 1024px) {
    .cv-container {
        grid-template-columns: 1fr;
    }

    .sidebar {
        position: static;
        width: 100%;
        height: auto;
    }

    .main-content {
